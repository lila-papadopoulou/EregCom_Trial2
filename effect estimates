*1.ATTENDANCE EFFECT ESTIMATES*
use "C:\Users\elpa\OneDrive - Folkehelseinstituttet\from personmap\Global Health\palestine rct\outcomes_04112021_nodup.dta"

gen clussize= str_TRIAL_2_ClusSize/100 if str_TRIAL_2_ClusSize!=.

codebook uniqueid phase_x b_TrialArm str_TRIAL_2_Cluster num_15_17 num_18_22 num_24_28 num_31_33 num_35_37 ///
denom_15_17 denom_18_22 denom_24_28 denom_31_33 denom_35_37 labavailability_y usavailability_y clussize str_TRIAL_2_ClusSize ///
agecat bookbmicat parity educationcat avgincomecat

keep uniqueid phase_x b_TrialArm str_TRIAL_2_Cluster num_15_17 num_18_22 num_24_28 num_31_33 num_35_37 ///
denom_15_17 denom_18_22 denom_24_28 denom_31_33 denom_35_37 labavailability_y usavailability_y clussize str_TRIAL_2_ClusSize ///
agecat bookbmicat parity educationcat avgincomecat

save "C:\Users\elpa\OneDrive - Folkehelseinstituttet\from personmap\Global Health\palestine rct\attendance.dta"
use "C:\Users\elpa\OneDrive - Folkehelseinstituttet\from personmap\Global Health\palestine rct\attendance.dta", clear

gen phase_n=1 if phase_x=="1"
replace phase_n=2 if phase_x=="2"
replace phase_n=3 if phase_x=="2 Gaza"

gen att1=num_15_17 if num_15_17!=.
gen att2=num_18_22 if num_18_22!=.
gen att3=num_24_28 if num_24_28!=. 
gen att4=num_31_33 if num_31_33!=.
gen att5=num_35_37 if num_35_37!=.

reshape long att, i(uniqueid) j(timepoint)
save "C:\Users\elpa\OneDrive - Folkehelseinstituttet\from personmap\Global Health\palestine rct\attendance_long.dta"
use "C:\Users\elpa\OneDrive - Folkehelseinstituttet\from personmap\Global Health\palestine rct\attendance_long.dta", clear

*A (0) vs. B (1)*
gen AB=0 if b_TrialArm=="A"
replace AB=1 if b_TrialArm=="B"
count if missing(AB)
assert r(N) == 0

*A (0) vs. C (1)*
gen AC=0 if b_TrialArm=="A"
replace AC=1 if b_TrialArm=="C"

*A (0) vs. D (1)*
gen AD=0 if b_TrialArm=="A"
replace AD=1 if b_TrialArm=="D"

*B (0) vs. C (1)*
gen BC=0 if b_TrialArm=="B"
replace BC=1 if b_TrialArm=="C"

*B (0) vs. D (1)* 
gen BD=0 if b_TrialArm=="B"
replace BD=1 if b_TrialArm=="D"

*C (0) vs. D (1)*
gen CD=0 if b_TrialArm=="C"
replace CD=1 if b_TrialArm=="D"

local TARMS "AB AC AD BC BD CD"
foreach x of local TARMS {
   xi:melogit i.att i.`x' i.timepoint i.phase_n i.us i.lab clussize || str_TRIAL_2_Cluster:|| uniqueid:, or
        }

